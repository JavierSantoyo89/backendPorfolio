generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tblbill {
  id_bill         Int      @id @default(autoincrement())
  folio           String
  no_orden_compra String
  emition_date    DateTime
  date_in         DateTime
  date_out        DateTime
  id_client       Int
  id_product      Int
  qty_finish      Int
}

model tblclient {
  id_client   Int    @id @default(autoincrement())
  company     String
  rfc         String
  address     String
  cp          String
  tel         String
  telcel      String
  fiscal_mode String
}

model tblemployee {
  id_employee Int    @id @default(autoincrement())
  user_name   String
  user_type   String
}

model tblhistoryprocess {
  id_history_process   Int      @id @default(autoincrement())
  name_history_process String
  qty                  Int
  date_history_process DateTime
  id_user              Int
}

model tblprocess {
  id_process         Int    @id @default(autoincrement())
  name_process       String
  id_history_process Int
}

model tblproduct {
  id_product   Int    @id @default(autoincrement())
  name_product String
  id_process   Int
  file_name    String
  price_unit   Int
}

model tblstadistics {
  id_stadistics Int       @id @default(autoincrement())
  status        String?   @db.VarChar(45)
  crud          String?   @db.VarChar(45)
  table         String?   @db.VarChar(45)
  project       String?   @db.VarChar(45)
  datetime      DateTime? @db.DateTime(0)
}
